{
    "terminal.integrated.fontSize": 10,
    "editor.wordWrap": "on",
    "files.autoSave": "afterDelay",
    "window.zoomLevel": 0.5,
    "editor.fontSize": 11,
    "workbench.colorTheme": "Atom One Dark",
    "workbench.iconTheme":"vscode-icons",
    


    "workbench.colorCustomizations": {
        "tab.activeBackground": "#077e03",
        "tab.inactiveBackground": "#0c0c0c",
        "titleBar.activeForeground": "#fffcfc",
        "tab.hoverBackground": "#3043ec",
        "tab.activeBorder": "#ff0000",
        "terminal.tab.activeBorder": "#33ff00",
        "tab.activeBorderTop": "#ff0000",
        "titleBar.activeBackground": "#000000",
        "C_Cpp.enhancedColorization": "Enabled",
        "C_Cpp.errorSquiggles": "Enabled",
        "C_Cpp.inlayHints": "Enabled"
    },
   
 "terminal.integrated.cursorStyle": "underline",
 "terminal.integrated.cursorBlinking": true,
 "terminal.integrated.tabs.title": "${process}",
"panelTitle.activeBorder": "#ffc600",
"files.associations": {
